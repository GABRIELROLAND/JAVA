package aula2;

public class CanetaTeste {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Caneta c1;
		c1=new Caneta();
		c1.cor="Vermelho";
		c1.tampada=false;
		c1.modelo="BIC";
		c1.status();
		c1.rabiscar();
    
    c2=new Caneta();
		c2.cor="Verde";
		c2.tampada=true;
		c2.modelo="COMPACTO";
		c2.status();
		c2.rabiscar();
	}

}
