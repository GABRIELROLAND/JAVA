package aula2;

public class Caneta {
 //Criaçao de atributos
	
	String modelo;
	String cor;
	float ponta;
	int carga;
	boolean tampada;	


	void rabiscar(){
		if(tampada){
			System.out.println("ERRO!!!A caneta esta tampada!");
		}
		else{
			System.out.println("Rabiscando...");
		}
		
	}
	
	void tampar(){
		
		
	}
	
	void destampar(){
		
		
		
	}
	
	void status(){
		System.out.println("Classe Caneta");
		System.out.println("Cor:" + cor);
		System.out.println("tampada:" + tampada);
		System.out.println("Modelo:" + modelo);
		
		
	}
	
	}
